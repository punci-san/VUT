./wedi ':' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi ' ' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi # <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi @ <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi '$' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi '&' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi '$' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi '*' <stdin >>stdout 2>>stderr; echo $? >>errcode
./wedi '(' <stdin >>stdout 2>>stderr; echo $? >>errcode
